{
    "plugins": ["jest"],
    "env": {
        "jest/globals": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes":"off",
        "no-unused-vars":"error",
        "indent":"error",
        "no-extra-semi":"warn",
        "jest/no-disabled-tests": "warn",
        "jest/no-focused-tests": "error",
        "jest/no-identical-title": "error",
        "jest/prefer-to-have-length": "warn",
        "jest/valid-expect": "error"
        
    }
}
